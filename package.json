{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --prod --aot=true && cp dist/timeline-tools/* .",
    "start": "http-server",
    "startprod": "ng serve -o --prod --aot=true",
    "build": "ng build --prod --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.5.3",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "ts-md5": "^1.2.7",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.0",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.18",
    "@types/xml2js": "^0.4.5",
    "codelyzer": "^5.2.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "postcss-less": "^3.1.4",
    "protractor": "^5.4.2",
    "stylelint": "^12.0.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": ">=3.4 < 3.6"
  }
}
