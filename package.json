{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --prod --aot=true && cp dist/timeline-tools/* .",
    "start": "http-server",
    "startprod": "ng serve -o --prod --aot=true",
    "build": "ng build --prod --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.4.0",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "ts-md5": "^1.2.9",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.11",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.0",
    "@angular/cli": "^12.2.13",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.2.11",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/jquery": "^3.5.8",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jquery": "^3.6.0",
    "karma": "~6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "~4.3.5"
  }
}
