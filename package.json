{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --configuration production && cp dist/timeline-tools/* .",
    "serve": "ng serve",
    "start": "http-server",
    "startprod": "ng serve -o --configuration production",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently -k -s first \"npm run serve\" \"npx playwright install && npx playwright test tests/\""
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "normalize.css": "8.0.1",
    "rxjs": "7.8.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "ts-md5": "1.3.1",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.11",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "14.2.11",
    "@angular/compiler-cli": "14.2.12",
    "@angular/language-service": "14.2.12",
    "@playwright/test": "1.31.2",
    "@types/jasmine": "4.3.1",
    "@types/jquery": "3.5.16",
    "@types/lodash-es": "4.17.7",
    "@types/node": "18.15.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "jquery": "3.6.4",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.5.0"
}
