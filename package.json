{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --prod --aot=true && cp dist/timeline-tools/* .",
    "start": "http-server",
    "startprod": "ng serve -o --prod --aot=true",
    "build": "ng build --prod --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "^11.0.3",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "^11.0.3",
    "@angular/compiler": "^11.0.3",
    "@angular/core": "^11.0.3",
    "@angular/forms": "^11.0.3",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "^11.0.3",
    "@angular/platform-browser-dynamic": "^11.0.3",
    "@angular/router": "^11.0.3",
    "moment": "^2.29.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.6.7",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "ts-md5": "^1.2.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.8",
    "@angular/cli": "^11.2.10",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/language-service": "^11.2.11",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "15",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jquery": "^3.6.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss-less": "^4.0.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.7"
  }
}
