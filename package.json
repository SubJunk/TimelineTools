{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --configuration production && cp dist/timeline-tools/* .",
    "serve": "ng serve",
    "start": "http-server",
    "startprod": "ng serve -o --configuration production",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently -k -s first \"npm run serve\" \"npx playwright install && npx playwright test tests/\""
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "15.2.9",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/material": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "normalize.css": "8.0.1",
    "rxjs": "7.8.1",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "ts-md5": "1.3.1",
    "tslib": "2.6.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.9",
    "@angular-eslint/builder": "15",
    "@angular-eslint/eslint-plugin": "15",
    "@angular-eslint/eslint-plugin-template": "15",
    "@angular-eslint/schematics": "15",
    "@angular-eslint/template-parser": "15",
    "@angular/cli": "15.2.9",
    "@angular/compiler-cli": "15.2.9",
    "@angular/language-service": "15.2.9",
    "@playwright/test": "1.35.1",
    "@types/jasmine": "4.3.5",
    "@types/jquery": "3.5.16",
    "@types/lodash-es": "4.17.7",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "concurrently": "8.2.0",
    "eslint": "8.43.0",
    "jasmine-core": "5.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "jquery": "3.7.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.6.0"
}
