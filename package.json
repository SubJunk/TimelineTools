{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --configuration production && cp dist/timeline-tools/* .",
    "serve": "ng serve",
    "start": "http-server",
    "startprod": "ng serve -o --configuration production",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently -k -s first \"npm run serve\" \"npx playwright install && npx playwright test tests/\""
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "^13.0.2",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/forms": "^13.0.2",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "@angular/router": "^13.0.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.1",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "ts-md5": "^1.2.10",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.1",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "^13.0.2",
    "@angular/language-service": "^13.1.1",
    "@playwright/test": "^1.17.1",
    "@types/jquery": "^3.5.11",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "^6.5.1",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jquery": "^3.6.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "stylelint": "^13.13.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "~4.4.4"
  }
}