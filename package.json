{
  "name": "timeline-tools",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve -o",
    "ng": "ng",
    "heroku-prebuild": "npm install -g @angular/cli http-server",
    "heroku-postbuild": "ng build --configuration production && cp dist/timeline-tools/* .",
    "serve": "ng serve",
    "start": "http-server",
    "startprod": "ng serve -o --configuration production",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently -k -s first \"npm run serve\" \"npx playwright install && npx playwright test tests/\""
  },
  "private": true,
  "stylelint": {
    "rules": {
      "block-opening-brace-space-before": "always",
      "declaration-block-semicolon-space-before": "never",
      "declaration-colon-space-after": "always",
      "indentation": 2
    }
  },
  "dependencies": {
    "@angular/animations": "13.3.9",
    "@angular/cdk": "13.3.7",
    "@angular/common": "13.3.9",
    "@angular/compiler": "13.3.9",
    "@angular/core": "13.3.9",
    "@angular/forms": "13.3.9",
    "@angular/material": "13.3.7",
    "@angular/platform-browser": "13.3.9",
    "@angular/platform-browser-dynamic": "13.3.9",
    "@angular/router": "13.3.9",
    "lodash-es": "4.17.21",
    "moment": "2.29.3",
    "normalize.css": "8.0.1",
    "rxjs": "7.5.5",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "ts-md5": "1.2.11",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.6",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.6",
    "@angular/compiler-cli": "13.3.9",
    "@angular/language-service": "13.3.9",
    "@playwright/test": "1.22.2",
    "@types/jasmine": "4.0.3",
    "@types/jquery": "3.5.14",
    "@types/lodash-es": "4.17.6",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "concurrently": "7.2.1",
    "eslint": "8.16.0",
    "jasmine-core": "4.1.1",
    "jasmine-spec-reporter": "7.0.0",
    "jquery": "3.6.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ts-node": "10.8.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  }
}
